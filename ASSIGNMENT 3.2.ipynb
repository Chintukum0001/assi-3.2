   "source": [
    "#Write a Python program to reverse a string.\n",
    "\n",
    "\n",
    "\n",
    "#Sample String : \"4321abcd\"\n",
    "\n",
    "#Expected Output : \"dcba1234\"\n",
    "\n",
    "\n",
    "def Reverse():\n",
    "    \n",
    "    R=input()\n",
    "    Q= list(R)\n",
    "    Q.reverse()\n",
    "    print(\"\".join(Q)) "
   ]
